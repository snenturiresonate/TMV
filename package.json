{
  "name": "tmvend-to-end-test-framework",
  "version": "0.5.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-ci": "protractor --baseUrl=\"http://$npm_config_ci_ip\" --cucumberOpts.tags=\"$cucumber_tags\" --params.dynamo_suffix=\"$dynamo_suffix\" e2e/protractor.conf.js",
    "fe2e": "TZ=Europe/London protractor --baseUrl=\"https://$npm_config_ci_ip\" --specs=\"$specs_to_run\" --cucumberOpts.tags=\"$cucumber_tags\" --params.dynamo_suffix=\"$dynamo_suffix\" --params.test_harness_ci_ip=\"http://$npm_config_test_harness_ip\" --params.redis_host=\"$npm_config_redis_host\" --params.redis_port=\"$npm_config_redis_port\" e2e/protractor.conf.js",
    "junit-xml": "cat .tmp/results.json | ./node_modules/.bin/cucumber-junit > .tmp/TEST-results.xml",
    "json-merge": "./node_modules/cucumber-json-merge/bin/cucumber-json-merge -d .tmp/ -o .tmp/results.json"
  },
